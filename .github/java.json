{
    "problemMatcher": [
        {
            "owner": "java",
            "pattern": [
                {
                    "regexp": "^\\s*at .*\\((.*):(\\d*)\\)$",
                    "file": 1,
                    "line": 2
                },
                {
                    "regexp": "^Exception in thread \"(.*)\" (.*): (.*)$",
                    "code": 2,
                    "message": 3
                }
            ]
        }
    ]
}